<?xml version="1.0" encoding="utf-8"?>
<map>
 <!-- マップ名 ※必須※ -->
 <name>絶壁指定都市 F U K U O K A</name>
 <!-- マップバージョン ※必須※ -->
 <version>1.8.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
        <author>njj12</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>10</time>
 
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>7</egg>
      <feather>3</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>2</egg>
      <bone>2</bone>
      <feather>2</feather>
 </rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>true</watch>
</map>
